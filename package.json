{
  "name": "alpheios-lexicon-client",
  "version": "1.1.0",
  "description": "Alpheios Lexicon Client Library",
  "main": "dist/alpheios-lexicon-client.js",
  "scripts": {
    "test": "standard --fix && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all all lib",
    "build-prod": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all production lib",
    "build-dev": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all development lib",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/morph-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/morph-client/issues"
  },
  "homepage": "https://github.com/alpheios-project/morph-client#readme",
  "devDependencies": {
    "alpheios-node-build": "git://github.com/alpheios-project/node-build",
    "alpheios-data-models": "git://github.com/alpheios-project/data-models",
    "babel-jest": "^21.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babyparse": "^0.4.6",
    "coveralls": "^3.0.0",
    "jest": "^21.2.1",
    "jest-environment-jsdom-11.0.0": "^20.1.9",
    "jest-fetch-mock": "1.3.3",
    "papaparse": "^4.3.6",
    "source-map-loader": "^0.2.3",
    "standard": "*"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-jsdom-11.0.0",
    "transformIgnorePatterns": [
      "node_modules/?!(alpheios-data-models)"
    ]
  },
  "standard": {
    "ignore": [
      "dist/**"
    ]
  },
  "dependencies": {}
}
